fn getElementAnzahl(arr: int[][]): int {
    let i = -1
    let amount = 0
    while ((i := i + 1) < arr.size) {
        amount += arr[i].size
    }
    return amount
}

fn indexOf(a: str[][], s: str): int[] {
    let result: int[] = null
    let x = -1
    while ((x := x + 1) < a.size && result == null) {
        let y = -1
        while ((y := y + 1) < a[x].size) if (a[x][y].equals(s)) {
             result = [x, y]
             break
        }
    }
    return result
}
