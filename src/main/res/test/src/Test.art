fn main() {
    fizzBuzz();
}

fn fizzBuzz() {
    let i = 0;
    while ((i := i + 1) <= 100) {
        let output = "";
        let isEmpty = true;
        if (i % 3 == 0) {
            output = "Fizz";
            isEmpty = false;
        }
        if (i % 5 == 0) {
            output += "Buzz";
            isEmpty = false;
        }
        if (isEmpty) print i; else print output;
//        i++;
    }
}

