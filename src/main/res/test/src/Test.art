fn main() {
    const printer: Printer = HelloWorldPrinter()
    printer.doPrint()
}

abstract class Printer {

    public abstract fn doPrint()

}


abstract class HelloPrinter : Printer {

    override public fn doPrint() {
        print this.getHello()
    }

    public abstract fn getHello(): str

}

class HelloWorldPrinter : HelloPrinter {

    public override fn getHello(): str {
        return "Hello World"
    }

}
